module segmentDisplay(
	input csi_clk,
	input rsi_reset_n,
	
	input avs_s0_write,
	input avs_s0_read,
	input[3:0] avs_s0_address,
	input[31:0] avs_s0_writedata,
	
	output reg[31:0] avs_s0_readdata
	
);

always@(*)begin
	avs_s0_readdata = 0;
	
	if(avs_s0_read)begin
		case(avs_s0_address)
			1: avs_s0_readdata = 32'd64;
			default: avs_s0_readdata = 32'hFFFFFFFF;
		endcase
	end
	
end

endmodule
